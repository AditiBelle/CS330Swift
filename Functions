//recursive function calucating factorial


func factorial() -> Int {
    
    let temp = 2
 
    
    
    if (temp <= 3){
        let temp = temp - 1
        
        
    if (temp >= 1) {
        let callingf = factorial()
                
        }

    }
    
    return temp
    
}
